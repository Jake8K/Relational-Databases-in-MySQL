                  <option value="4">flexibility</option>
                  <option value="9">maintaining</option>
                  <option value="8">mindfulness</option>
                  <option value="10">physical therapy</option>
                  <option value="7">recovery</option>
                  <option value="3">strength</option>
                  <option value="11">vanity</option>
                  <option value="6">weight loss</option>
